---
dnote:
  service: DNote
  active: true

ri:
  service : Ri
  output  : ri
  include : lib
  active  : false

rdoc:
  service : RDoc
  template: twofish
  output  : site/doc/rdoc
  title   : Erbside API
  main    : README.rdoc
  extra   : ""
  include : [README.rdoc, lib]
  active  : false

yard:
  service : Yard
  output  : site/doc/api
  title   : Erbside
  readme  : README.core
  include : [lib, README.rdoc, HISTORY.rdoc, APACHE2.txt, NOTICE.rdoc]
  active  : false

